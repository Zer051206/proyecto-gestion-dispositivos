{
  "source": {
    "include": [
      "server/src/models",
      "server/src/controllers",
      "server/src/routes"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/jsdoc/templates/default"
  },
  "plugins": []
}
